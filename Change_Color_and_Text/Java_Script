function change() {
  var div1 = document.getElementById("div1");
  div1.classList.toggle("colour");
  var div2 = document.getElementById("div2");
  div2.classList.toggle("colour_1");
}

function changetxt() {
  var div1 = document.getElementById("div1");
  var div2 = document.getElementById("div2");
  div1.innerHTML = "The boys";
  div2.innerHTML = "The girls";
}
